<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE pkgmetadata SYSTEM "http://www.gentoo.org/dtd/metadata.dtd">
<pkgmetadata>
	<maintainer type="person">
		<email>csmk@chaoslab.org</email>
		<name>Ian Moone</name>
	</maintainer>
	<longdescription lang="en">
		KeePassXC is a fork of KeePassX that aims to incorporate
		stalled pull requests, features, and bug fixes that have
		never made it into the main KeePassX repository.
	</longdescription>
	<use>
		<flag name="autotype">Enable the autotype feature</flag>
		<flag name="http">Enable the KeePassHTTP protocol</flag>
	</use>
	<upstream>
		<remote-id type="github">keepassxreboot/keepassxc</remote-id>
	</upstream>
</pkgmetadata>
