name="OnionCat"
pidfile="${PIDFILE}"
command="/usr/bin/ocat"
command_args="-P ${PIDFILE} ${OCAT_OPTS}"
command_background=yes
start_stop_daemon_args="--user $NAME --group $NAME"

depend() {
	use net
	need tor
}
