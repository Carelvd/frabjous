: ${UG:="onioncat:onioncat"}
: ${PIDFILE:="/run/ocat.pid"}

name="OnionCat"
command="/usr/bin/ocat"
command_args="-P ${PIDFILE} ${OCAT_OPTS}"
command_background=yes
pidfile="${PIDFILE}"
start_stop_daemon_args="-u ${UG}"

depend() {
	use net
	need tor
}
